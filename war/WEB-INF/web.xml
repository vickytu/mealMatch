<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>CreateMealMatchServlet</servlet-name>
		<servlet-class>mealmatch.CreateMealMatchServlet</servlet-class>
	</servlet>

	<servlet-mapping>
	  <servlet-name>CreateMealMatchServlet</servlet-name>
	  <url-pattern>/main</url-pattern>
	</servlet-mapping>

<servlet>
    <servlet-name>CollegeCreateServlet</servlet-name>
    <servlet-class>mealmatch.CollegeCreateServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CollegeCreateServlet</servlet-name>
    <url-pattern>/create</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>StudentCheckin</servlet-name>
    <servlet-class>mealmatch.StudentCheckin</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>StudentCheckin</servlet-name>
    <url-pattern>/checkin</url-pattern>
  </servlet-mapping>

   <servlet>
    <servlet-name>StudentCheckout</servlet-name>
    <servlet-class>mealmatch.StudentCheckout</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>StudentCheckout</servlet-name>
    <url-pattern>/checkout</url-pattern>
  </servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
